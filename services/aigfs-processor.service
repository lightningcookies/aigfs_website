[Unit]
Description=AIGFS Data Processor
After=aigfs-downloader.service

[Service]
Type=simple
User=lenovo1
WorkingDirectory=/home/lenovo1/Documents/aigfs/aigfs_website
Environment=PYTHONUNBUFFERED=1
ExecStart=/home/lenovo1/Documents/aigfs/aigfs_website/venv/bin/python backend/processor.py
Restart=always
RestartSec=60

[Install]
WantedBy=multi-user.target
